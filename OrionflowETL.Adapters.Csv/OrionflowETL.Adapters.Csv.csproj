<Project Sdk="Microsoft.NET.Sdk">


	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>

		<PackageId>OrionflowETL.Adapters.Csv</PackageId>
		<Version>0.1.3</Version>
		<Authors>Absalón Hernández</Authors>
		<Description>
			CSV adapter for OrionflowETL. Provides an ISource implementation
			to read CSV files into ETL pipelines.
		</Description>
		<PackageTags>etl;csv;orionflow</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/ikaros14/OrionflowETL</RepositoryUrl>

		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
    <ProjectReference Include="..\OrionflowETL.Core\OrionflowETL.Core.csproj" />
	</ItemGroup>

</Project>
